[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    cia = commit --amend
    lg = log --graph --pretty=format:'%C(yellow)%h %Creset~ %ce%n✒ %Cblue%s%n⌚ %ci %n'
    df = diff
    fo = fetch origin
    fu = fetch upstream
    pom = push origin master
    pod = push origin develop
    mom = merge origin/master
    mod = merge origin/develop
[color]
    branch = auto
    status = auto
    diff = auto
[color "branch"]
    current = green
[color "status"]
    untracked = red
    added = green
    changed = yellow
